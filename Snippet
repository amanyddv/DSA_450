#include <bits/stdc++.h>
using namespace std;
 
#define  forn(i,n,j)  for(int i=j;i<n;i++)
#define  form(j,n)  for(int j=n-1;j>0;j--)
#define pb push_back
#define ll long long
int s;
 
// __gcd(x,y)
 
void solve(){
    int n,m;
    cin>>n>>m;
    vector<ll> a(n+m);
    forn(i,n+m,0) cin>>a[i];
    
    
    sort(a.begin(),a.end()-1);
 
    reverse(a.begin(),a.end());
 
    ll ans = 0;
    forn(i,n,0)ans += a[i];
 
    cout<<ans<<endl;
 
    
}
 
 
int main () {
  int T;
  cin >> T;
  while (T--) {
     solve();
    
    
  }
  return 0;
}
